<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
    http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
	version="2.0">

	<persistence-unit name="reporte">
		<class>com.savvis.spirits.report.model.domain.AttributeMapping</class>
		<class>com.savvis.spirits.report.model.domain.ColumnMetadata</class>
		<class>com.savvis.spirits.report.model.domain.ComplexModel</class>
		<class>com.savvis.spirits.report.model.domain.DatabaseType</class>
		<class>com.savvis.spirits.report.model.domain.Datasource</class>
		<class>com.savvis.spirits.report.model.domain.Model</class>
		<class>com.savvis.spirits.report.model.domain.ModelQuery</class>
		<class>com.savvis.spirits.report.model.domain.SimpleModel</class>
		<properties>
			<property name="hibernate.dialect" value="org.hibernate.dialect.MySQLDialect" />
			<property name="hibernate.show_sql" value="true" />
		</properties>
	</persistence-unit>
</persistence>